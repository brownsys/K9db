filegroup(
    name = "lobsters-data",
    srcs = glob([
        "lobsters/*.sql",
        "lobsters/*.txt",
    ]) + [
        "//experiments/lobsters:schema/schema.sql",
        "//experiments/lobsters:schema/views.sql",
        "//experiments/lobsters:schema/extra-views.sql",
    ],
    visibility = ["//tests:__subpackages__"],
)

filegroup(
    name = "medical-chat-data",
    srcs = glob([
        "medical/*.sql",
        "medical/*.txt",
    ]),
    visibility = ["//tests:__subpackages__"],
)
